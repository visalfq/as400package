<div class="pui-cloud-all">
  <div class="pui-cloud-row">
    <div class="pui-cloud-column">
      <div class="pui-cloud-user-workspaces">
        <ul>
          <% for (let i = 0; i < spaces.length; i++) { %>
          <%   const space = spaces[i]; %>
            <li class="pui-ws-container">

              <a class="pui-ws-link" href="/ide/<%= encodeURIComponent(space.dir) %>/" target="_blank"><%= space.dir %></a>
              <img src="/profoundui/proddata/images/remove.png" class="pui-cloud-space-remove" title="Remove space" onclick="pui.ide['removeWorkspace']('<%= space.dir %>');"/>

              <div class="pui-cloud-list-desc"><%= space.settings.description %></div>

            </li>
          <% } %>
        </ul>

        <% if (spaces.length <= 0) { %>
          None
          (<a href="javascript:pui.ide['createWorkspace']();">create</a>)
        <% } %>
      </div>
    </div>
  </div>
</div>
