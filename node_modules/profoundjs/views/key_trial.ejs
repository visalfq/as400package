<!doctype html>
<html>

<head>
  <meta charset="utf-8" />
  <meta http-equiv="x-ua-compatible" content="ie=edge" />
  <meta name="viewport" content="width=device-width, initial-scale=1" />
  <title>Request Profound Logic Trial</title>
  <style>
    @font-face {
      font-family: 'Source Sans Pro';
      src: url(/profoundui/proddata/fonts/OpenSansV6.woff);
    }
    
    .pjs-key-trial-page {
      background: #082e40;
      height: 95vh;
    } 
    
    .pjs-key-trial-page * {
      box-sizing: border-box;
      outline: none;
    }
    
    .pjs-key-trial-page-container {
      display: flex;
      align-items: center;
      align-content: center;
      justify-items: center;
      justify-content: center;
      height: 100%;
      width: 100%;
    }
    
    .pjs-key-trial-content-container {
      display: flex;
      flex-flow: row nowrap;
      align-items: center;
      align-content: center;
      justify-items: center;
      justify-content: center;
      color: #b3b3b3;
      font-family: 'Source Sans Pro', sans-serif;
      border: 2px solid #b3b3b3;
      width: 800px;
      height: 450px;
      border-radius: 10px;
    }
    
    .pjs-key-trial-inner {
      height: calc(100% - 15px);
      width: calc(100% - 15px);
      border: 1px solid rgb(243, 118, 33);
      display: flex;
      border-radius: 10px;
      position: relative;
    }
    
    .pjs-key-trial-title {
      font-weight: bold;
      font-size: 13px;
      display: flex;
      align-content: center;
      align-items: center;
      justify-content: center;
      justify-items: center;
      flex-flow: column nowrap;
      color: white;
      flex-grow: 1;
      border-right: 1px solid rgb(243, 118, 33);
      width: 50%;
      margin-top: 30px;
      margin-bottom: 30px;
    }
    
    .pjs-key-trial-title img {
      max-width: 300px;
    }
    
    .pjs-key-trial-error {
      color: #FF0001;
      font-size: 13px;
      text-align: right;
      position: absolute;
      width: 763px;
      right: 5px;
      bottom: 5px;
      margin-right: 3px;
      margin-left: 13px;
      overflow: auto;
      padding: 2px;
      max-height: 25px;
    }
    
    .pjs-key-trial-form {
      width: 50%;
      display: flex;
      flex-grow: 4;
    }
    
    .pjs-key-trial-form div h2 {
      width: 100%;
      text-align: center;
      margin: 0;
      color: white;
    }
    
    .pjs-key-trial-form div {
      width: 100%;
      display: flex;
      align-content: center;
      align-items: center;
      justify-content: center;
      justify-items: center;
      flex-grow: 1;
    }
    
    .pjs-key-trial-form form {
      width: 100%;
      display: flex;
      flex-flow: column;
      justify-content: center;
    }
    
    .pjs-key-trial-form input {
      width: 320px;
      height: 40px;
      display: inline-block;
      color: white;
      font-size: 13px;
      background: 0;
      border: 2px solid #b3b3b3;
      border-radius: 10px;
      padding-left: 10px;
      padding-right: 10px;
    }
    
    .pjs-key-trial-form input:focus {
      border-color: rgb(243, 118, 33);
    }
    
    .pjs-key-trial-form .pjs-key-trial-submit {
      height: 35px;
      position: relative;
    }
    
    .pjs-key-trial-form .pjs-key-trial-submit input {
      border: none;
      background: rgb(243, 118, 33);
      color: white;
      font-size: 15px;
      height: 40px;
      cursor: pointer;
      overflow: hidden;
      border-radius: 10px;
      letter-spacing: 1.9px;
      text-transform: uppercase;
    }
    
    .pjs-key-trial-form-names input {
      width: 150px;
      margin-left: 10px;
      margin-right: 10px;
    }
    
    .pjs-key-trial-form .pjs-key-trial-submit input:active {
      transform: translateY(1px);
    }
  </style>
</head>

<body class="pjs-key-trial-page" onload="<%=(typeof message!="undefined")?`alert("${message}")`:" "%>">
  <div class="pjs-key-trial-page-container">
    <div class="pjs-key-trial-content-container">
      <div class="pjs-key-trial-inner">
        <div class="pjs-key-trial-title">
          <img src="/profoundui/proddata/images/keys/Profound_Logic_Color_Logo.png" />
        </div>
        <div class="pjs-key-trial-form">
          <form method="post" action="/key/trial">
            <div></div>
            <div>
              <h2>Request Trial Form</h2>
            </div>
            <div class="pjs-key-trial-form-names"><input placeholder="First Name" type="text" id="first" name="first" value="<%= (typeof first != "undefined") ? first : "" %>" required autofocus /><input placeholder="Last Name" type="text" id="last" name="last" value="<%= (typeof last != "undefined") ? last : "" %>" required /></div>
            <div>
              <input placeholder="Company" type="text" id="company" name="company" value="<%= (typeof company != "undefined") ? company : "" %>" required /></div>
            <div>
              <input placeholder="email@emailhost.com" type="email" id="email" name="email" value="<%= (typeof email != "undefined") ? email : "" %>" required /></div>
            <div>
              <input placeholder="Phone Number" type="tel" id="phone" name="phone" value="<%= (typeof phone != "undefined") ? phone : "" %>" required /></div>
            <div class="pjs-key-trial-submit">
              <input type="submit" value="Submit &#10148;" /></div>
            <div></div>
          </form>
        </div>
        <% if (typeof error != "undefined") { %>
        <div class="pjs-key-trial-error"><span>
            <%= error %></span></div>
        <% } %>

      </div>
    </div>
  </div>
</body>

</html>